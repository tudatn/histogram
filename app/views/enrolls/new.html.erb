<%= stylesheet_link_tag "enrolls" %>

<%= link_to("<< Back to List", enrolls_path, :class => 'back-link') %>

<div class="enrolls new">
  <h2>Create Enrollment</h2>

  <%= form_for(@enroll) do |f| %>
    <%= render(:partial => 'form', :locals => {:f => f}) %>
    <div class="form-buttons">
      <%= f.submit("Create Enrollment") %>
    </div>
  <% end %>
</div>
