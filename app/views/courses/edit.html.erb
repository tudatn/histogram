<%= link_to("<< Back to List", courses_path, :class => 'back-link') %>

<div class="courses edit">
  <h2>Update Course</h2>

  <%= form_for(@course) do |f| %>
    <%= render(:partial => 'form', :locals => {:f => f}) %>
    <div class="form-buttons">
      <%= f.submit("Update Course") %>
    </div>
  <% end %>
</div>
